{
  "name": "mojidoodle-algo-segmenter",
  "version": "0.0.2",
  "description": "Gap-based Japanese handwriting stroke segmenter",
  "author": {
    "name": "Lexa Baldwin",
    "email": "lexa.40@proton.me",
    "url": "https://github.com/Lexa-B",
    "orcid": "https://orcid.org/0000-0002-3990-2875"
  },
  "homepage": "https://lexa-b.github.io/MojiDoodle-Algo-Segmenter/",
  "repository": "https://github.com/Lexa-B/MojiDoodle-Algo-Segmenter",
  "bugs": "https://github.com/Lexa-B/MojiDoodle-Algo-Segmenter/issues",
  "license": "MIT",
  "keywords": [
    "Japanese",
    "handwriting",
    "segmenter",
    "gap-based",
    "online handwriting recognition"
  ],
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "deploy": "gh workflow run deploy-example.yml && echo 'Workflow triggered. Watch: gh run watch $(gh run list --workflow=deploy-example.yml --limit=1 --json databaseId -q .[0].databaseId)'"
